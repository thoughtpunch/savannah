# AI Savannah (Savannah) — Environment Configuration
# Copy to .env and fill in your values:  cp .env.sample .env

# ── LLM Provider API Keys ────────────────────────────────────────
# Only set the keys for providers you plan to use.

# Anthropic API (for provider: "anthropic_api" or litellm with anthropic/ prefix)
ANTHROPIC_API_KEY=

# OpenAI API (for litellm with openai/ prefix or gpt-* models)
OPENAI_API_KEY=

# Google AI (for litellm with gemini/ prefix)
GOOGLE_API_KEY=

# Groq (for litellm with groq/ prefix)
GROQ_API_KEY=

# Together AI (for litellm with together_ai/ prefix)
TOGETHER_API_KEY=

# ── Local Models ─────────────────────────────────────────────────
# Ollama base URL (for litellm with ollama/ prefix)
# OLLAMA_BASE_URL=http://localhost:11434

# ── Python Environment ───────────────────────────────────────────
# Uncomment to control Python behavior
# PYTHONDONTWRITEBYTECODE=1
# PYTHONUNBUFFERED=1
